{% extends "layout.njk" %}


{% block head %}
<title>{{item.title}}</title>
{% endblock %}

{% block content %}

<div class="page">
    <img src="https://skylab.rocketseat.com.br/static/{{item.image}}.svg">
    <h1>{{item.title}}</h1>
    <h2>{{item.description}}</h2>
    <div class="page__info">
        <p>{{item.length}}</p>
        <p>{{item.price}}</p>
    </div>
</div>

{% endblock %}